    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "XLSX convert",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": [
                "./example/input/example.xlsx",
                "./example/output/example"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "XLS convert",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": [
                "./example/input/example.xls",
                "./example/output/example"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CSV convert",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/index.js",
            "args": [
                "./example/input/example.csv",
                "./example/output/example"
            ]
        }
    ]
